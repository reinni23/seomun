<main>
  <section class="bg1">
    <div class="bg_img1"></div>
    <div class="bg_txt1">
      <p>공지사항</p>
    </div>
  </section>
  <section class="write">
    <h2>공지사항 작성</h2>
    <% if(typeof errs !== 'undefined') { %> <% errs.forEach((err)=>{%>
    <%=err.param%> : <%=err.msg%> <% })%> <% } %>
    <form method="POST" action="/store">
      <div>
        <ul>
          <li class="write_title">제목</li>
          <li><input type="text" name="title" class="lab_title" /></li>
        </ul>
        <ul>
          <li class="write_content">내용</li>
          <li>
            <textarea type="text" name="content"  class="lab_cont"></textarea>
          </li>
        </ul>
        <ul>
          <li>
            <input type="submit" value="저장" id="save" class="btn_cl"/>
            <input
              type="button"
              value="뒤로 가기"
              onclick="javascript:location.href='notice'"
              class="btn_cl"
            />
          </li>
        </ul>
      </div>
    </form>
  </section>
</main>
